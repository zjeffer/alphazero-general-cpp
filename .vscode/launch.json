  "version": "0.2.0",
  "configurations": [
    {
      "name": "C++ CMake Debug",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/build/alphazero",
      "args": [
        "--model",
        "models/model_trained.pt",
        "--network-architecture",
        "config/architectures/tic-tac-toe.jsonc",
        "--game-config",
        "config/game/tic-tac-toe.jsonc",
        "--data-folder",
        "data/data02"
      ],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "miDebuggerPath": "/usr/bin/gdb"
    }
  ]
}